{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "seniorpink"
		},
		"AzureDataLakeStorageInputLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageInputLinkedService'"
		},
		"AzureDataLakeStorageInputLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://freefinalstorageaccount.dfs.core.windows.net/"
		},
		"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://donflamingo.dev.azuresynapse.net"
		},
		"RefinePatientTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2075028d-e3d3-4030-84e2-bb118eb5f397/resourceGroups/Main_Resource/providers/Microsoft.Storage/storageAccounts/freefinalstorageaccount"
		},
		"RefineDoctorTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2075028d-e3d3-4030-84e2-bb118eb5f397/resourceGroups/Main_Resource/providers/Microsoft.Storage/storageAccounts/freefinalstorageaccount"
		},
		"RefineVisitTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2075028d-e3d3-4030-84e2-bb118eb5f397/resourceGroups/Main_Resource/providers/Microsoft.Storage/storageAccounts/freefinalstorageaccount"
		},
		"RefineTreatmentTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2075028d-e3d3-4030-84e2-bb118eb5f397/resourceGroups/Main_Resource/providers/Microsoft.Storage/storageAccounts/freefinalstorageaccount"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "seniorpink_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageInputLinkedService_accountKey": {
						"value": "[parameters('AzureDataLakeStorageInputLinkedService_accountKey')]"
					},
					"AzureDataLakeStorageInputLinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageInputLinkedService_properties_typeProperties_url')]"
					},
					"AzureSynapseArtifacts1_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureSynapseArtifacts1_properties_typeProperties_endpoint')]"
					},
					"RefinePatientTrigger_properties_typeProperties_scope": {
						"value": "[parameters('RefinePatientTrigger_properties_typeProperties_scope')]"
					},
					"RefineDoctorTrigger_properties_typeProperties_scope": {
						"value": "[parameters('RefineDoctorTrigger_properties_typeProperties_scope')]"
					},
					"RefineVisitTrigger_properties_typeProperties_scope": {
						"value": "[parameters('RefineVisitTrigger_properties_typeProperties_scope')]"
					},
					"RefineTreatmentTrigger_properties_typeProperties_scope": {
						"value": "[parameters('RefineTreatmentTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "seniorpink_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'seniorpink_ArmTemplate_0')]"
			]
		}
	]
}